
public class Movie {

	private final String title;
	private final String releaseDate;
	private final String runningTime;
	private final String review;

	public Movie(String title, String releaseDate, String runningTime, String review) {
		this.title = title;
		this.releaseDate = releaseDate;
		this.runningTime = runningTime;
		this.review = review;
	}

	public void getInfo() {
		System.out.println(
				"Фільм:" + title + "\rДата виходу:" + releaseDate + "\rТривалість:" + runningTime + "\rОпис:" + review);
	}
}
